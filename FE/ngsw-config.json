{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name":"Backend ApI",
      "urls": [
        "http://127.0.0.1:8000/game/*",
        "http://images.igdb.com/igdb/*"
      ],
      "cacheConfig":{
        "maxSize": 10,
        "maxAge":"12h",
        "timeout":"10s",
        "strategy":"performance"
      }
    }
  ]
}
